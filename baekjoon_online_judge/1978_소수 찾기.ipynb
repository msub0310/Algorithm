    "# 내 풀이\r\n",
    "nums = [1, 3, 5, 7]\r\n",
    "#_ = input()\r\n",
    "#nums = list(map(int, input().split()))\r\n",
    "nums = list(set(nums))\r\n",
    "count = 0\r\n",
    "for num in nums:\r\n",
    "    if num == 1:\r\n",
    "        continue\r\n",
    "    if all([num % i != 0 for i in range(2, (num // 2) + 1)]):\r\n",
    "        count += 1\r\n",
    "print(count)"
